<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomPicker">
        <attr name="custom_picker_data" format="reference" />
        <attr name="custom_picker_selected_item_position" format="integer" />
        <attr name="custom_picker_item_text_size" format="dimension" />
        <attr name="custom_picker_item_text_color" format="color" />
        <attr name="custom_picker_selected_item_text_color" format="color" />
        <attr name="custom_picker_same_width" format="boolean" />
        <attr name="custom_picker_maximum_width_text" format="string" />
        <attr name="custom_picker_maximum_width_text_position" format="integer" />
        <attr name="custom_picker_visible_item_count" format="integer" />
        <attr name="custom_picker_item_space" format="dimension" />
        <attr name="custom_picker_cyclic" format="boolean" />
        <attr name="custom_picker_indicator" format="boolean" />
        <attr name="custom_picker_indicator_color" format="color" />
        <attr name="custom_picker_indicator_size" format="dimension" />
        <attr name="custom_picker_curtain" format="boolean" />
        <attr name="custom_picker_curtain_color" format="color" />
        <attr name="custom_picker_atmospheric" format="boolean" />
        <attr name="custom_picker_curved" format="boolean" />
        <attr name="custom_picker_item_align" format="enum">
            <enum name="center" value="0" />
            <enum name="left" value="1" />
            <enum name="right" value="2" />
        </attr>
        <attr name="custom_picker_font_path" format="string" />
    </declare-styleable>
</resources>